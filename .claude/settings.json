{
  "permissions": {
    "allow": [
      "Bash",
      "Read",
      "Write",
      "Edit",
      "Glob",
      "Grep",
      "WebFetch",
      "WebSearch",
      "mcp__coder__*",
      "mcp__playwright__*"
    ],
    "deny": [
      "Bash(git push --force*)",
      "Bash(git push -f*)",
      "Bash(git push * --force*)",
      "Bash(git push * -f*)",
      "Bash(git push origin main*)",
      "Bash(git push origin master*)",
      "Bash(git checkout main)",
      "Bash(git checkout master)",
      "Edit(.claude/settings.json)",
      "Write(.claude/settings.json)"
    ]
  }
}
